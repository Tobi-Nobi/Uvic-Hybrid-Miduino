// Reading for AUTOMOTIVE CURRENT TRANSDUCER DHAB S/18

#define GLV_MIN 0
#define GLV_MAX 1023

#define GLV_PIN ## // signal for LEM_DHAB
byte GLV_Data[1] = {0x00};


void GLV_status();

GLV_status():
{
  // Reads voltage output from sensor
  int GLV_current_read = analogRead(GLV_PIN);
  int GLV_correction_read = GLV_current_read - GLV_MIN

  
  int GLV_Percent = (100 * GLV_correction_read) / (GLV_MAX - GLV_MIN);
  GLV_Data[0] = GLV_Percent & 0xff;
  
}
